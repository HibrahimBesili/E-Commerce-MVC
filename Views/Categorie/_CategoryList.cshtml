@using E_Commerce_MVC.Models
@model IEnumerable<CategoryModel>
<li><a href="/Home/Product"><span class="icon-chevron-right"></span>Ürünler</a></li>

@foreach (var item in Model)
{
    <li><a href="/Home/ProductList/@item.Id"><span class="icon-chevron-right"></span>@item.Name [@item.Count]</a></li>

}
